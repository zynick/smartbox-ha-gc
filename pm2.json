{
    "apps": [{
        "name": "ha-gc",
        "script": "app.js",
        "watch": false,
        "log_date_format": "YYYY-MM-DD HH:mm Z",
        "out_file": "/var/log/ha-gc-out.log",
        "error_file": "/var/log/ha-gc-err.log",
        "env": {
            "DEBUG": "app:*",
            "NODE_ENV": "development"
        },
        "env_production": {
            "DEBUG": "app:*",
            "NODE_ENV": "production"
        }
    }]
}
